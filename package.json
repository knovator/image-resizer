{
    "name": "@knovator/image-resizer",
    "version": "0.1.0",
    "license": "MIT",
    "author": "knovator",
    "main": "dist/index.js",
    "module": "dist/image-resizer.esm.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src",
        "package.json",
        "README.md"
    ],
    "scripts": {
        "analyze": "size-limit --why",
        "build": "dts build",
        "lint": "dts lint",
        "prepare": "dts build",
        "size": "size-limit",
        "start": "dts watch",
        "test": "dts test"
    },
    "husky": {
        "hooks": {
            "pre-commit": "dts lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4
    },
    "jest": {
        "testEnvironment": "node"
    },
    "peerDependencies": {},
    "engines": {
        "node": ">=12"
    },
    "size-limit": [
        {
            "path": "dist/image-resizer.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/image-resizer.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^8.0.0",
        "@tsconfig/recommended": "^1.0.1",
        "@types/express": "^4.17.13",
        "@types/glob": "^7.2.0",
        "@types/sharp": "^0.30.4",
        "dts-cli": "^1.5.2",
        "husky": "^8.0.1",
        "size-limit": "^8.0.0",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "aws-sdk": "^2.1220.0",
        "glob": "^8.0.3",
        "sharp": "^0.30.7"
    }
}
